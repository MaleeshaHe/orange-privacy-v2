{
  "name": "orangeprivacy-mvp",
  "version": "1.0.0",
  "description": "OrangePrivacy MVP - Facial Recognition Privacy Tool",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:backend": "cd backend && npm install --production",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "db:migrate": "cd backend && npm run migrate",
    "db:migrate:undo": "cd backend && npm run migrate:undo"
  },
  "keywords": [
    "facial-recognition",
    "privacy",
    "aws-rekognition",
    "mvp"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
